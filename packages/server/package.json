{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "dev:build": "tsc --project tsconfig.build.json --watch",
    "dev:run": "nodemon --watch dist --exec \"node dist/index.js\"",
    "dev": "concurrently \"npm run dev:build\" \"npm run dev:run\"",
    "kill-port": "kill-port 8000",
    "start": "node dist/index.js",
    "lint": "eslint . --ext ts --cache",
    "lint:fix": "eslint . --ext ts --cache --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio",
    "db:setup": "node --loader ts-node/esm prisma/setup-database.js"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@graphql-tools/schema": "^10.0.2",
    "@prisma/client": "^5.10.0",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.2",
    "whatwg-mimetype": "^4.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "concurrently": "^9.1.2",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "nodemon": "^2.0.21",
    "prisma": "^5.10.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
