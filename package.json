{
  "name": "typescript-react-app",
  "version": "1.0.0",
  "description": "Node.js app with TypeScript, React, ESLint, and Jest",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "ts-node-dev --project src/server/tsconfig.json --respawn --transpile-only src/server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p src/server/tsconfig.json",
    "build:client": "vite build",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "dotenv": "^16.3.1",
    "graphql-ws": "^5.14.2",
    "ws": "^8.16.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "@types/dotenv": "^8.2.0",
    "@types/ws": "^8.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "jsdom": "^23.0.1"
  }
}
